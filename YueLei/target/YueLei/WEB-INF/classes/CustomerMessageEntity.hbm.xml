<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.yuelei.entity.CustomerMessageEntity" table="customer_message" schema="lay">
        <id name="messageId" column="message_id"/>
        <property name="username" column="username"/>
        <property name="sex" column="sex"/>
        <property name="phone" column="phone"/>
        <property name="messageDate" column="message_date"/>
        <property name="status" column="status"/>
        <property name="messageContent" column="message_content"/>
    </class>
</hibernate-mapping>