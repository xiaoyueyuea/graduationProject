<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.yuelei.entity.FinancialManagerEntity" table="financial_manager" schema="lay">
        <id name="financialStatementsId" column="financial_statements_id"/>
        <property name="orderId" column="order_id"/>
        <property name="roomType" column="room_type"/>
        <property name="roomPrice" column="room_price"/>
        <property name="startDate" column="start_date"/>
        <property name="endDate" column="end_date"/>
        <property name="days" column="days"/>
        <property name="roomCharge" column="room_charge"/>
        <property name="priceDifference" column="price_difference"/>
        <property name="otherCost" column="other_cost"/>
    </class>
</hibernate-mapping>